<body class="{{page}}" data-uid="{{uid}}">

    <div class="container-fluid">
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <a class="navbar-brand" href="/">
                        <img src="/images/logo_sm.png" />
                    </a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="/" class="home"><span><strong>Welcome</strong> {{profile.twitter.username}}</span></a></li>
                    {{#if isAuth}}
                    <!--<li><a class="btn btn-default navbar-btn" href="/profile">Profile</a></li>-->
                    <li><a class="btn btn-default navbar-btn" href="/logout">Logout</a></li>
                    {{else}}
                    <li><a href="/auth/twitter" class="btn btn-default navbar-btn">Login Twitter</a></li>
                    {{/if}}
                </ul>
            </div>
        </nav>
        <div class="column col-sm-3 col-xs-3" id="sidebar">
            <div class="buttons">
                <a href="#" class="glyphicon glyphicon-remove close-btn"></a>
            </div>
            <div style="clear:both"></div>
            <div class="inner">
            </div>
        </div>
        <div class="container-fluid" id="content">
            {{{body}}}
        </div>
        <div class="map">
            <div class="loader">
                <div class="loader-inner">
                    <p></p>
                    <div class="ball-pulse">
                        <div></div>
                        <div></div>
                        <div></div>
                    </div>
                </div>
                <div class="bck"></div>
            </div>
            <div id="map-canvas"></div>
        </div>
        <footer>
        </footer>
    </div>
